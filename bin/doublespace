#!/usr/bin/python
import sys
import fractions
f = sys.stdin if len(sys.argv) == 1 else open(sys.argv[1])
lines = f.readlines()
indent = [len(line) - len(line.lstrip(" ")) for line in lines]
unit = reduce(fractions.gcd, [i for i in indent if i > 0], 2)
assert(indent > 1);

for line in lines:
	print line.rstrip().replace("\t", "  ").replace(" " * unit, "  ")
	print
